<openerp>
    <data>
        <template id="vhr_base.assets_backend" name="vhr base assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/vhr_base/static/src/js/view_form.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/search.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/data.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/view_list.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/views.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/dashboard.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/format.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/chrome.js"></script>
                <script type="text/javascript" src="/vhr_base/static/src/js/vhr_base.js"></script>
                <link rel="stylesheet" href="/vhr_base/static/src/fonts/font-awesome-4.3.0/css/font-awesome.min.css" /> 
                <link rel="stylesheet" href="/vhr_base/static/src/css/vhr_base.css"/>
            </xpath>
       </template>
       <template id="vhr_base.assets_common" name="vhr base common assets" inherit_id="web.assets_common">
            <xpath expr="//script[@src='/web/static/lib/qweb/qweb2.js']" position="before">
            	<script type="text/javascript" src="/vhr_base/static/src/js/lib/jquery.freezeheader.js"></script>
            </xpath>
        </template>
    </data>
</openerp>
